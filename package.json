{
  "name": "root",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "clean": "lerna clean",
    "deploy": "npm run build && lerna publish",
    "tidy": "lerna run tidy",
    "ncu": "lerna exec -- ncu -u && npm run bootstrap && npm run build",
    "ncu:minor:deep": "npm run clean:npm && ncu -u --semverLevel major && npm install && lerna exec -- ncu '/^@mishguru//' -u --semverLevel major && lerna clean -y && npm run bootstrap && npm run build",
    "publish": "echo '===========================================================================\nWarning: `npm run publish` has been deprecated. Please use `npm run deploy`\n===========================================================================' && npm run deploy"
  },
  "devDependencies": {
    "lerna": "3.15.0"
  },
  "dependencies": {
    "@mishguru/fanout-helpers": "18.2.7",
    "@mishguru/fanservice": "14.5.0",
    "@mishguru/turbine": "5.0.0"
  }
}
